apiVersion: v1
clusters:
- cluster:
    server: {{ eks_info['cloudformation']['microblog-eks']['stack_outputs']['ClusterEndpoint'] }}
    certificate-authority-data: {{ eks_info['cloudformation']['microblog-eks']['stack_outputs']['ClusterCA'] }}
  name: microblog
contexts:
- context:
    cluster: eks-microblog
    user: jenkins
  name: ansible
current-context: ansible
kind: Config
preferences: {}
users:
- name: jenkins
  user:
    exec:
      apiVersion: client.authentication.k8s.io/v1alpha1
      command: aws-iam-authenticator
      args:
        - "token"
        - "-i"
        - "microblog-eks"
         - "-r"
         - "{{ ansible_facts['ec2_iam_info_instanceprofilearn'] }}"